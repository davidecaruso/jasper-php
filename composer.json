{
    "name": "davidecaruso/jasper-php",
    "description": "PHP library to execute Jasper Reports",
    "type": "library",
    "require-dev": {
        "codeception/codeception": "^2.3",
        "codeception/aspect-mock": "^2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Caruso",
            "email": "davide.caruso93@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JasperPHP\\": "src/JasperPHP/"
        }
    },
    "require": {
        "php": ">=7.0.0"
    },
    "scripts": {
        "require-jasper-starter": [
            "php -r \"exec('./scripts/require-jasper-starter.sh');\""
        ],
        "post-package-install": [
            "@require-jasper-starter"
        ],
        "post-package-update": [
            "@require-jasper-starter"
        ]
    }
}
